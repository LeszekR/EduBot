<div class="question" data-questionid="40079214"  id="question">

    <div id="dfp-tlb" class="everyonelovesstackoverflow"></div>    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
        <input type="hidden" name="_id_" value="40079214">
