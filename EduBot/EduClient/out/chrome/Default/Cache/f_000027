<div id="answer-43912244" class="answer" data-answerid="43912244"  itemscope itemtype="http://schema.org/Answer">
    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
        <input type="hidden" name="_id_" value="43912244">
