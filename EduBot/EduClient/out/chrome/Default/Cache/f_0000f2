<div id="answer-27504411" class="answer" data-answerid="27504411"  itemscope itemtype="http://schema.org/Answer">
    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
        <input type="hidden" name="_id_" value="27504411">
        <a class="vote-up-off" title="This answer is useful">up vote</a>
