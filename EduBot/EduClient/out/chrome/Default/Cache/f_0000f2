                <div id="answers-header">
                    <div class="subheader answers-subheader">
                        <h2>
                                7 Answers
                                <span style="display:none;" itemprop="answerCount">7</span>
                        </h2>
                        <div>
                            <div id="tabs">
        <a href="/questions/27504256/mvc-web-api-no-access-control-allow-origin-header-is-present-on-the-requested?answertab=active#tab-top" data-nav-xhref="" title="Answers with the latest activity first" data-value="active" data-shortcut="A">
